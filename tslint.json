{
  "class-name": true,
  "comment-format": [
    true, "check-space"
  ],
  "indent": [
    true, "tabs"
  ],
  "no-duplicate-variable": true,
  "no-eval": true,
  "no-internal-module": true,
  "no-var-keyword": true,
  "semicolon": true,
  "triple-equals": [
    true, "allow-null-check"
  ],
  "variable-name": [
    true, "ban-keywords", "check-format"
  ]
}
